<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.31.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1762509676138-1" author="younes">
        <addColumn tableName="delivery_history">
            <column name="delivery_id" type="BIGINT"/>
        </addColumn>
    </changeSet>
    <changeSet id="1762509676138-2" author="younes">
        <addNotNullConstraint columnName="delivery_id" tableName="delivery_history"/>
    </changeSet>
    <changeSet id="1762509676138-3" author="younes">
        <addForeignKeyConstraint baseColumnNames="delivery_id" baseTableName="delivery_history"
                                 constraintName="FK_DELIVERY_HISTORY_ON_DELIVERY" referencedColumnNames="id"
                                 referencedTableName="delivery"/>
    </changeSet>

</databaseChangeLog>